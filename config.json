{
    "name": "barneyman deploy server",
    "version": "1.8.6",
    "slug": "barneyman_deploy",
    "description": "Fetches built binaries from Git (R and PR) and pushes them to local devices",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "before",
    "boot": "auto",
    "hassio_api": false,
    "homeassistant_api": false,
    "hassio_role": "default",
    "options": { 
        
        "host":"hassio", 
        "port":8080,
        "logging":"WARNING",
        "prerelease":false,
        "release":true,
        "legacy":true,
	"poll":15
    },
    "schema": { 
        "host":"str", 
        "port":"int(0,65535)",
        "logging":"list(CRITICAL|ERROR|WARNING|INFO|DEBUG)",
        "prerelease":"bool",
        "release":"bool",
        "legacy":"bool",
	"poll":"int(5,120)"
    },
    "ports_description":{"8080/tcp":"RESTful endpoint"},
    "ports": {
        "8080/tcp": 8080
    }
}
